<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sicdlib.dto">
    <class name="ParamType" table="param_type">
        <id name="id" type="java.lang.String" column="PARAM_TYPE_ID">
            <generator class="uuid"/>
        </id>
        <property name="name" column="PARAM_TYPE_NAME" length="1000" />
        <property name="desc" column="PARAM_TYPE_DESC" length="1000" />
    </class>
</hibernate-mapping>