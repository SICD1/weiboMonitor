<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sicdlib.dto">
	<class name="RolePrivilege" table="tb_role_privilege">
		<id name="rp_id" type="java.lang.String" column="rp_id">
			<generator class="uuid"/>
		</id>
		<property name="role_id" type="java.lang.String" column="role_id" />
		<property name="privilege_id" type="java.lang.String" column="privilege_id" />
	</class>
</hibernate-mapping>

