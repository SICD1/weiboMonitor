<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sicdlib.dto">
	<class name="Group" table="tb_group">
		<id name="g_id" type="java.lang.String" column="g_id">
			<generator class="uuid"/>
		</id>
		<property name="g_name" type="java.lang.String" column="g_name"/>
		<property name="g_parent_id" type="java.lang.String" column="g_parent_id"  />
		<property name="role_id" type="java.lang.String" column="role_id" />
	</class>
</hibernate-mapping>

