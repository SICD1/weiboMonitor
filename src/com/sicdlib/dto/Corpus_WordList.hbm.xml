<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sicdlib.dto">
	<class name="Corpus_WordList" table="tb_corpus_wordlist">
		<id name="cwl_id" type="java.lang.Integer" column="cwl_id">
			<generator class="uuid"/>
		</id>
		<property name="cwl_word"   type="java.lang.String"  column="cwl_word"/>
		<property name="cwl_occurTimes"   type="java.lang.Integer"  column="cwl_occurTimes"/>
		<property name="cwl_occurFrequency"   type="java.lang.Double"  column="cwl_occurFrequency"/>
		<property name="cwl_accumFrequency"   type="java.lang.Double"  column="cwl_accumFrequency"/>
	</class>
</hibernate-mapping>

