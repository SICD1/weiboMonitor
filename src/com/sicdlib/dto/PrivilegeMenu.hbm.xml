<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sicdlib.dto">
	<class name="PrivilegeMenu" table="tb_privilege_menu">
		<id name="mp_id" type="java.lang.String" column="mp_id">
			<generator class="uuid"/>
		</id>

		<many-to-one name="menu" class="Menu" column="m_id" cascade="all" fetch="select"/>
		<many-to-one name="privilege" class="Privilege" column="p_id" cascade="all" fetch="select"/>
	</class>
</hibernate-mapping>

