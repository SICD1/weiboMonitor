<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sicdlib.dto">
	<class name="PrivilegeMenu" table="tb_privilege_menu">
		<id name="mp_id" type="java.lang.String" column="mp_id">
			<generator class="uuid"/>
		</id>
		<property name="menu_id" type="java.lang.String" column="menu_id" />
		<property name="privilege_id" type="java.lang.String" column="privilege_id" />

	</class>
</hibernate-mapping>

