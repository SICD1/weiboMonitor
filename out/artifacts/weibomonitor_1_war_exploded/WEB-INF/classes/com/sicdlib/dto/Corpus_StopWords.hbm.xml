<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sicdlib.dto">
	<class name="Corpus_StopWords" table="tb_corpus_stopwords">
		<id name="csd_id" type="java.lang.Integer" column="csd_id">
			<generator class="uuid"/>
		</id>
		<property name="csd_word"   type="java.lang.String"  column="csd_word" />
	</class>
</hibernate-mapping>

