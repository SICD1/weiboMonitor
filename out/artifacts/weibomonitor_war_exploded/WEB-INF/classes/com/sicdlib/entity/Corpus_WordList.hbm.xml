<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sicdlib.entity">
	<class name="Corpus_WordList" table="tb_corpus_wordlist">
		<id name="cwl_id">
			<generator class="native" />
		</id>
		<property name="cwl_word" length="40" />
		<property name="cwl_occurTimes" type="integer"/>
		<property name="cwl_occurFrequency" type="double"/>
		<property name="cwl_accumFrequency" type="double"/>
	</class>
</hibernate-mapping>

