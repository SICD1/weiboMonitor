<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sicdlib.entity">
	<class name="AuthFunctionList" table="tb_authfunctionlist">
		<id name="afl_id">
			<generator class="native" />
		</id>
		<property name="afl_name" length="50" />
		<property name="afl_code" length="20" />
		<property name="afl_filterURLprefix" length="50" />
		<property name="afl_parentID" length="20" />
		<!-- 多个功能列表属于一个权限与功能列表中间表 -->
		<many-to-one name="authority_AuthFunctionList" column="a_afl_id"/>
		
	</class>
</hibernate-mapping>

